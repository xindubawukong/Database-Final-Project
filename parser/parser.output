Grammar

    0 $accept: program $end

    1 program: %empty
    2        | program stmt

    3 stmt: sysStmt ';'
    4     | dbStmt ';'
    5     | tbStmt ';'
    6     | idxStmt ';'
    7     | alterStmt ';'
    8     | QUIT

    9 sysStmt: SHOW DATABASES

   10 dbStmt: CREATE DATABASE dbName
   11       | DROP DATABASE dbName
   12       | USE dbName
   13       | SHOW TABLES

   14 tbStmt: CREATE TABLE tbName '(' fieldList keyConstraints ')'
   15       | DROP TABLE tbName
   16       | DESC tbName

   17 keyConstraints: %empty
   18               | ',' keyConstrint keyConstraints

   19 keyConstrint: PRIMARY '(' columnList ')'
   20             | FOREIGN '(' columnList ')' REFERENCES tbName '(' columnList ')'

   21 idxStmt: CREATE INDEX idxName ON tbName '(' columnList ')'
   22        | DROP INDEX idxName

   23 alterStmt: ALTER TABLE tbName ADD PRIMARY '(' columnList ')'
   24          | ALTER TABLE tbName DROP PRIMARY
   25          | ALTER TABLE tbName ADD CONSTRAINT fkName FOREIGN '(' columnList ')' REFERENCES tbName '(' columnList ')'
   26          | ALTER TABLE tbName DROP FOREIGN fkName
   27          | ALTER TABLE tbName ADD columnName field
   28          | ALTER TABLE tbName DROP columnName
   29          | ALTER TABLE tbName CHANGE columnName field

   30 fieldList: field
   31          | fieldList ',' field

   32 field: columnName type
   33      | columnName type NOTNULL
   34      | columnName type DEFAULT value
   35      | columnName type NOTNULL DEFAULT value

   36 type: T_INT '(' VALUE_INT ')'
   37     | VARCHAR '(' VALUE_INT ')'
   38     | T_FLOAT

   39 value: VALUE_INT
   40      | VALUE_STRING
   41      | VALUE_FLOAT
   42      | VALUE_NULL

   43 columnList: columnName
